{
  "name": "valaux",
  "version": "1.2.4",
  "private": true,
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node --max-old-space-size=4096 scripts/build.js",
    "docker-image": "yarn build && docker build -t oamdev/velaux:latest -f Dockerfile.local .",
    "test": "node scripts/test.js",
    "lint": "yarn lint:js && yarn lint:style && yarn lint:prettier",
    "lint:js": "eslint --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "lint:prettier": "prettier -c --write \"src/**/*\" --end-of-line auto",
    "precommit": "lint",
    "tsc": "tsc --noEmit",
    "mock": "node scripts/mock.js"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --fix"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "eslintConfig": {
    "parser": "@babel/eslint-parser"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@alifd/meet-react": "^2.0.0",
    "@antv/g6": "4.3.11",
    "@b-design/ui": "^1.0.63",
    "ansi-to-react": "^6.1.6",
    "axios": "0.24.0",
    "diagram-maker": "^1.3.0",
    "dva": "2.4.1",
    "dva-core": "2.0.4",
    "dva-loading": "3.0.22",
    "echarts": "^5.2.2",
    "i18next": "^19.8.2",
    "js-yaml": "^4.1.0",
    "lodash": "4.17.21",
    "moment": "^2.27.0",
    "monaco-editor": "0.30.0",
    "prop-types": "^15.7.2",
    "rax": "^1.1.0",
    "react": "^16.3.0",
    "react-cookies": "^0.1.1",
    "react-copy-to-clipboard": "^5.0.4",
    "react-dnd": "^7.3.2",
    "react-dnd-html5-backend": "^7.2.0",
    "react-dom": "^16.3.0",
    "react-i18next": "11.13.0",
    "react-markdown": "7.1.0",
    "react-refresh": "^0.10.0",
    "redux": "4.1.2",
    "remark-gfm": "3.0.1",
    "tiny-pubsub": "^1.1.0",
    "tsx-control-statements": "4.1.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.13.5",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.1",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^26.0.24",
    "@types/react-copy-to-clipboard": "^5.0.2",
    "@types/react-dom": "^17.0.9",
    "@types/js-yaml": "^4.0.1",
    "@types/lodash": "^4.14.176",
    "@types/react": "^16.3.0",
    "@types/react-cookies": "^0.1.0",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^4.0.0",
    "@typescript-eslint/parser": "^4.0.0",
    "@umijs/fabric": "2.8.1",
    "babel-loader": "8.2.3",
    "bfj": "7.0.2",
    "browserslist": "4.17.6",
    "case-sensitive-paths-webpack-plugin": "2.3.0",
    "css-loader": "5.2.7",
    "dotenv": "10.0.0",
    "dotenv-expand": "5.1.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-formatter-pretty": "4.0.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "25.2.3",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^3.9.2",
    "eslint-webpack-plugin": "^2.5.2",
    "file-loader": "6.1.1",
    "fs-extra": "10.0.0",
    "html-webpack-plugin": "4.5.2",
    "less": "3.9.0",
    "less-loader": "4.1.0",
    "mini-css-extract-plugin": "0.11.3",
    "optimize-css-assets-webpack-plugin": "6.0.1",
    "pnp-webpack-plugin": "1.6.4",
    "postcss": "^8.3.3",
    "postcss-flexbugs-fixes": "4.2.1",
    "postcss-loader": "3.0.0",
    "postcss-normalize": "8.0.1",
    "postcss-preset-env": "6.7.0",
    "postcss-safe-parser": "6.0.0",
    "react-dev-utils": "^11.0.4",
    "resolve-url-loader": "4.0.0",
    "sass-loader": "^10.0.5",
    "style-loader": "1.3.0",
    "ts-loader": "8.1.0",
    "typescript": "4.4.4",
    "url-loader": "4.1.1",
    "webpack": "4.44.2",
    "webpack-dev-server": "3.11.1",
    "webpack-manifest-plugin": "2.2.0",
    "workbox-webpack-plugin": "5.1.4"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testRunner": "./node_modules/jest-circus/runner.js",
    "transform": {
      "^.+\\.(js|jsx|mjs|cjs|ts|tsx)$": "<rootDir>/config/jest/babelTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|cjs|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|cjs|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "resetMocks": true
  }
}